{
  "alias": "zc.phar",
  "chmod": "0755",
  "directories": [
    "src/Transformer/Pcre"
  ],
  "files": [
    "LICENSE",
    "dist/version",
    "vendor/autoload.php",
    "src/constants.php",
    "src/Reader/AbstractReader.php",
    "src/Reader/File.php",
    "src/Reader/GzipResource.php",
    "src/Reader/StandardIn.php",
    "src/Transformer/String/ContainsFilter.php",
    "src/Writer/AbstractWriter.php",
    "src/Writer/StandardOut.php"
  ],
  "finder": [
    {
      "name": "*Interface.php",
      "in": "src/Reader"
    },
    {
      "name": "*.php",
      "in": "src/Transformer",
      "depth": 0
    },
    {
      "name": "*Interface.php",
      "in": "src/Writer"
    },
    {
      "name": "*.php",
      "in": "bin"
    },
    {
      "name": "autoload_*.php",
      "in": "vendor/composer"
    },
    {
      "name": "*Loader.php",
      "in": "vendor/composer"
    }
  ],
  "git-version": "package_version",
  "main": "bin/zc",
  "output": "dist/zc.phar",
  "stub": true
}
